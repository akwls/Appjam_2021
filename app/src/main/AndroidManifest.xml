<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.appjam2021">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".PasswordActivity"></activity>
        <activity android:name=".MyInfoPartyInfoActivity" />
        <activity android:name=".PartyJoinActivity" />
        <activity android:name=".NewPartyActivity" />
        <activity android:name=".MoreInfoActivity" />
        <activity android:name=".JoinPasswordActivity" />
        <activity android:name=".MyInfoActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".SplashActivity"
            android:label="@string/app_name"
            android:theme="@style/SplashTheme">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>